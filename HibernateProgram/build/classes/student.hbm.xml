<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


    <hibernate-mapping>
	
	<class name="com.example.Student" table="hellostudent">
	<id name="id" column="sid"></id>
	<property name="name" column="sname" not-null="false" />
	<property name="degree" column="sdegree" />
	<property name="phonenumber" column="sphonenumber" />
	<property name="email" column="semail" />
	
	</class>
	</hibernate-mapping>